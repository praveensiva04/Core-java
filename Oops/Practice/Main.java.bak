		this.name=newname;	
	}
	
	Student()
	{
		System.out.println("VANAKAM");
		this.name="JAAAAVID";
		this.age=23;
		this.marks=67.3f;
		this("Praveen",22,78.5f);
	}
	void Greet()
	{
		System.out.println("VANAKAaaaM");
	}
	Student(Student other)
	{
		this.name=other.name;
		this.age=other.age;
		this.marks=other.marks;
	}
	Student(String name,int age,float marks)
	{
		this.name=name;
		this.age=age;
		this.marks=marks;
	}
}
